{
    "pipe_type": "beam",
    "pipe_config": {
        "inputs": [
            {
                "kind": "json",
                "input_file": "pipe_gaps/data/sample_messages_lines.json",
                "lines": true
            }
        ],
        "side_inputs": [
            {
                "kind": "query",
                "query_name": "gaps",
                "query_params": {
                    "source_gaps": "scratch_tomas_ttl30d.pipe_ais_gaps",
                    "start_date": "2012-01-01"
                },
                "mock_db_client": false
            }
        ],
        "core": {
            "kind": "detect_gaps",
            "threshold": 1,
            "show_progress": false,
            "eval_last": true,
            "normalize_output": true
        },
        "outputs": [
            {
                "kind": "bigquery",
                "table": "scratch_tomas_ttl30d.pipe_ais_gaps_new_schema",
                "description": "Gaps for AIS position messages.",
                "schema": "gaps",
                "write_disposition": "WRITE_APPEND"
            }
        ],
        "options": {
            "runner": "direct",
            "region": "us-east1",
            "network": "gfw-internal-network",
            "subnetwork": "regions/us-east1/subnetworks/gfw-internal-us-east1"
        }
    }
}