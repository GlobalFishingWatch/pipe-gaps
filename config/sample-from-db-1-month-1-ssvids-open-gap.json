{
    "pipe_type": "naive",
    "pipe_config": {
        "inputs": [
            {
                "kind": "query",
                "query_name": "messages",
                "query_params": {
                    "source_messages": "pipe_ais_v3_published.messages",
                    "source_segments": "pipe_ais_v3_published.segs_activity",
                    "start_date": "2024-03-01",
                    "end_date": "2024-04-01",
                    "ssvids": [412331104]
                },
                "mock_db_client": false
            }
        ],
        "side_inputs": [
            {
                "kind": "json",
                "schema": "ais_gaps",
                "input_file": "pipe_gaps/data/open_gaps.json",
                "lines": true
            }
        ],
        "core": {
            "kind": "detect_gaps",
            "threshold": 1,
            "show_progress": false,
            "eval_last": true
        },
        "outputs": [
            {
                "kind": "json",
                "output_prefix": "gaps"
            }
        ],
        "options": {
            "runner": "direct",
            "region": "us-east1",
            "network": "gfw-internal-network",
            "subnetwork": "regions/us-east1/subnetworks/gfw-internal-us-east1"
        }
    }
}