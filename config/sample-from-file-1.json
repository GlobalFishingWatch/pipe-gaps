{
    "pipe_type": "beam",
    "pipe_config": {
        "inputs": [
            {
                "kind": "json",
                "schema": "messages",
                "input_file": "pipe_gaps/data/sample_messages.json"
            }
        ],
        "side_inputs": [
            {
                "kind": "json",
                "schema": "gaps",
                "input_file": "pipe_gaps/data/open_gaps.json",
                "lines": true
            }
        ],
        "core": {
            "threshold": 1.2,
            "show_progress": false,
            "eval_last": true
        },
        "options": {
            "runner": "direct"
        },
        "save_json": true
    }
}