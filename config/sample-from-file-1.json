{
    "pipe_type": "beam",
    "pipe_config": {
        "inputs": [
            {
                "kind": "json",
                "schema": "messages",
                "input_file": "pipe_gaps/data/sample_messages.json"
            }
        ],
        "side_inputs": [
            {
                "kind": "json",
                "schema": "ais_gaps",
                "input_file": "pipe_gaps/data/open_gaps.json",
                "lines": true
            }
        ],
        "core": {
            "kind": "detect_gaps",
            "threshold": 1.2,
            "show_progress": false,
            "eval_last": true,
            "normalize_output": true
        },
        "outputs": [
            {
                "kind": "json",
                "output_prefix": "gaps"
            }
        ],
        "options": {
            "runner": "direct"
        }
    }
}